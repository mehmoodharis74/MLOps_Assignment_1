pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'Building...'
            }
        }
        
        stage('Dev') {
            steps {
                echo 'Running in Dev environment...'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Running tests..'
            }
        }
    }
}